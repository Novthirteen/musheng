		<id name="Id" column="Id" type="Int32" unsaved-value="0" >
      		<generator class="identity" />
        </id>
		<property name="Item" column="Item" type="String" length="50" />	
		<property name="Uom" column="Uom" type="String" length="5" />	
		<property name="Customer" column="Customer" type="String" length="50" />	
		<property name="Qty" column="Qty" type="Double"  />	
		<property name="Balance" column="Balance" type="Double"  />	
		<property name="FinanceCalendar" column="FinanceCalendar" type="String" length="7" />	
		<property name="CreateDate" column="CreateDate" type="DateTime"  />	
		<property name="CreateUser" column="CreateUser" type="String" length="50" />	
  	</class>
</hibernate-mapping>
