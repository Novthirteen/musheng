<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
 	<class name="com.Sconit.Entity.Cost.CostCenter, com.Sconit.Entity" table="CostCenter" lazy="false">
		<id name="Code" column="Code" type="String" unsaved-value="null" >
      		<generator class="assigned" />
        </id>
		<property name="Description" column="Desc1" type="String" length="255" />
    <property name="IsActive" column="IsActive" type="Boolean"  />
		<many-to-one name="CostGroup" column="CostGroup" class="com.Sconit.Entity.Cost.CostGroup, com.Sconit.Entity" cascade="none" />
  	</class>
</hibernate-mapping>
